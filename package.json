{
  "name": "huckleberry-mentorship-bot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "start": "tsx src/bot/index.ts",
    "start:prod": "node dist/bot/index.js",
    "register": "tsx src/bot/registerCommands.ts",
    "webhook": "tsx src/server/webhookServer.ts",
    "webhook:prod": "node dist/server/webhookServer.js",
    "start:all": "tsx src/start.ts",
    "start:all:prod": "node dist/start.prod.js",
    "stop:all": "pm2 stop ecosystem.config.js",
    "restart:all": "pm2 restart ecosystem.config.js",
    "push:all": "git push origin main && git push gitlab main",
    "lint": "eslint \"src/**/*.{ts,tsx,js}\"",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:audit:force": "npm audit --audit-level=moderate",
    "security:outdated": "npm outdated"
  },
  "dependencies": {
    "@discordjs/builders": "^1.13.0",
    "@discordjs/rest": "^2.6.0",
    "@supabase/supabase-js": "^2.80.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "discord-api-types": "^0.38.32",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "resend": "^6.4.2",
    "tsx": "^4.20.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  }
}
